}
/*幻灯片CSS开始*/
.Carousel {
	width:740px;
	height:350px;
	margin:0 auto;
	margin-top: 25px;
	position: relative;
}
.Carousel_main{
	width:100%;
	position: absolute;
	left:0;
	top:0;
}
.Carousel_main li a{
	display: block;
	width: 740px;
	height: 350px;
}
.Carousel_main li img {
	display:block;
	width:100%;
	height:350px;
	position: absolute;
	left:0;
	top:0;
}
.Carousel_span {
	width: 740px;
	height:35px;
	position: absolute;
	left:0;
	bottom:0;
	zoom:1;
}
.Carousel_span span {
	width:15px;
	height:15px;
	display:block;
	float:left;
	margin-left:10px;
	background: url(../images/dot.png) no-repeat left bottom;
}
.Carousel_span p {
	width:100px;
	height:35px;
	margin:0 auto;
}
.Carousel_span .Carousel_span_one{
	background: url(../images/dot.png) no-repeat left top;
}
.Carousel_left {
	width:57px;
	height:57px;
	cursor: pointer;
	background:#000 url(../images/ad_ctr.png) no-repeat 5px -15px;
	filter:alpha(opacity:50);opacity:0.5;
	position: absolute;
	left:50px;
	top:150px;
	display:none;
}
.Carousel_right {
	width:57px;
	height:57px;
	cursor: pointer;
	background:#000 url(../images/ad_ctr.png) no-repeat -5px -105px;
	filter:alpha(opacity:50);opacity:0.5;
	position: absolute;
	right:50px;
	top:150px;
	display:none;
}

