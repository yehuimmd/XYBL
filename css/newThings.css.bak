nav{
	width: 100%;
	height: 50px;
	background: #bbd441;
	text-align: center;
}
nav li{
	height: 100%;
	padding: 0 30px;
	float: left;
	line-height: 50px;
	color: #fff;
	font-weight: bold;
	font-size: 24px;
	cursor: pointer;
}
nav>ul{
	display: inline-block;
}
nav li:hover{
