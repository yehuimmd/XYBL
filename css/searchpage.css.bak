*{
	margin: 0;
	padding-left: 0;
	text-decoration: none;
	list-style: none;
	font-family: 微软雅黑;
}
html,body{
	text-align: center;
}
/*头部*/
.header{
	width: 100%;
	height: 51px;
	margin-bottom: 59px;
	margin-top: 30px;
}
.header_content,.nav_content{
	width: 1000px;
	height: 100%;
	margin: 0 auto;
	position: relative;
}
.header_content_block{
	width: 160px;
	height: 100%;
	background-color: #bed742;
	display:block;
	float: left;
}
#Search_input{
	width: 520px;
	height: 35px;
	float: left;
	margin-top: 12px;
	margin-left: 30px;
	border-right: none;
}
#Search_span{
	width: 98px;
	height: 38px;
	border: none;
	background-color: #bed742;
	border-radius: 7px;
	cursor: pointer;
	margin-top: 12px;
	margin-left: -8px;
}
.head {
	float: left;
	position: absolute;
	right: 0;
	top: 0;
	font-size: 16px;
}
.head li{
	list-style: none;
	float: left;
	margin-left: 28px;
}
.head li a{
	color: #999999;
	text-decoration: none;
}
/*登录界面*/
#close{
	width: 25px;
	height: 25px;
	float: right;
}
#log_card{
	text-align: center;
	display: none;
	z-index: 101;
	margin: 0 auto;
	position: absolute;
	top: 200px;
	left: 50%;
	margin-left:-257px; 
	width: 515px;
	height:324px;
	background: #FFFFFF;
}
.log_card_log{
	margin-top: 58px;
	font-family: '微软雅黑';
	font-size: 24px;
	font-weight: bold;
}
#userName{
	display: inline-block;
	margin-top: 30px;
	margin-right: 20px;
	font-family: '微软雅黑';
	font-size: 18.67px;
	font-weight: bold;
}
#password{
	display: inline-block;
	margin-top: 24px;
	margin-right: 20px;
	font-family: '微软雅黑';
	font-size: 18.67px;
	font-weight: bold;
}
.userName{
	width: 241px;
	height: 27px;
}
.password{
	width: 241px;
	height: 27px;
}
.log_btn{
	margin-top: 35px;
	margin-left: 160px;
	width: 241px;
	height: 42px;
	line-height: 42px;
	border-radius: 10px;
	cursor: pointer;
	font-family: '微软雅黑';
	font-size: 18.67px;
	font-weight: bold;
	background-color: #BED742;
}
.searchpage_wrap{
	height:840px;
}
.searchpage_box{
	margin:0 auto;
	width:1000px;
	height:830px;
}
.searchpage_contain{
	margin-top:30px;
	width:1000px;
	height:150px;
	border:1px solid #e7e7e7;
}
.searchpage_contain:hover{
	background-color:#EAEC64;
	border-color:#EAEC64;
}
.searchpage_contain h3{
	font-size:19px;
	margin-left:60px;
	margin-top:10px;
    float:left;
    text-decoration:underline;
    color:#333333;
}
.searchpage_contain img{
	float:left;
	margin-left:-120px;
 	margin-top:40px;
	width:120px;
	height:95px;
}
.searchpage_contain p{
	text-align:left;
	line-height:30px;
	width:705px;
	height:145px;
	float:right;
	margin-right:60px;
	color:#666666;
}
.page{
	width: 100%;
	height: 35px;
	display: block;
	margin-bottom: 60px;
	margin-top: 30px;
	text-align: center;
	position: relative;
}
.page_ul{
	overflow: hidden;
	display: inline-block;
}
.page_li,.page_change{
	margin: 0;
	padding: 0;
	width: 33px;
	height: 33px;
	float: left;
	list-style: none;
	margin-right: 16px;
	border: 1px solid #e7e7e7;
	font-size: 16px;
	font-weight: bold;
	color: #666;
	line-height: 33px;
	text-align: center;
	cursor: pointer;
}
.page .page_last{
	margin-right: 0;
}
.page .page_change{
	border: 1px solid #e7e7e7;
	font-size: 16px;
	font-weight: bold;
	color: #666;
	line-height: 33px;
	text-align: center;
	width: 73px;
	height: 33px;
}
.page .page_next{
	position: absolute;
	right: 140px;
}
.page .page_prev{
	position: absolute;
	left: 140px;
	
}
.page_change:hover,.page li:hover{
	background-color: #eaec63;
	color: #fff;
}
.pageColor{
	background-color: #eaec63;
	color: #fff; 
}
.mancenter_bottom{
	margin-top:35px;
	height:90px;
 }
.mancenter_hr{
	margin:0 auto;
    width:850px;
 }
.mancenter_bottom_container{
	margin:0 auto;
	width:1000px;
	height:90px;
 }
.mancenter_bottom_container p{
	padding-top:15px;
 	font-family:微软雅黑;
	font-size:16px;
	text-align:center;
	color:#999999;
}
/*遮罩层*/
#mask {
	position: absolute; 
	top: 0px;left: 0px;
	display: none;
	width: 100%;
	/*height: 100%;*/
	-moz-opacity: 0.7; 
	opacity:.70; 
	filter: alpha(opacity=70); 
	background-color: #777;
	z-index: 100;
}
